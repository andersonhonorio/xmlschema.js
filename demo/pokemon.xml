<trainer xmlns="https://paulwardrip.github.io/xmlschema.js/demo/pokemon"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://paulwardrip.github.io/xmlschema.js/demo/pokemon
            https://paulwardrip.github.io/xmlschema.js/demo/pokemon.xsd">

    <pokeball inventory="2" />
    <potion inventory="1" />
    <revive inventory="3" />

    <team>
        <!-- valid pokemon -->
        <pokemon id="1" level="4" hp="100" hpmax="100">
            <name>Bulbasaur</name>
            <type>poison</type>
            <type>gras</type>
        </pokemon>

        <!-- invalid pokemon -->
        <pokemon id="1">
            <name>Bulbasaur1</name>
            <type>poison</type>
            <type>grass</type>
            <type>water</type>
        </pokemon>
    </team>
</trainer>